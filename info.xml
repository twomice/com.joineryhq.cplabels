<?xml version="1.0"?>
<extension key="com.joineryhq.cplabels" type="module">
  <file>cplabels</file>
  <name>CarePartners Custom Mailing Labels</name>
  <description>Custom Mailing Labels for CarePartners</description>
  <license>GPL-3.0</license>
  <maintainer>
    <author>Allen Shaw</author>
    <email>allen@JoineryHQ.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/twomice/com.joineryhq.cplabels</url>
    <url desc="Documentation">https://github.com/twomice/com.joineryhq.cplabels</url>
    <url desc="Support">https://github.com/twomice/com.joineryhq.cplabels/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/gpl-3.0.html</url>
  </urls>
  <releaseDate>2019-04-24</releaseDate>
  <version>1.0.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.27</ver>
  </compatibility>
  <comments>Requires com.joineryq.labelsort</comments>
  <civix>
    <namespace>CRM/Cplabels</namespace>
    <format>25.01.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.3</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
